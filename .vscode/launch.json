  "version": "0.2.0",
  "configurations": [
    {
      "name": "Go Main",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "sticker",
      "env": {
        "PORT": "4000",
        "DB_STRING": "root:sticker-pass@tcp(localhost:3306)/stickers",
        "JWT_TOKEN": "SECRET-TOKEN"
      }
    },
    {
      "name": "Go Debug",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "main.go",
      "env": {
        "PORT": "4000",
        "DB_STRING": "root:sticker-pass@tcp(localhost:3306)/stickers",
        "JWT_TOKEN": "SECRET-TOKEN"
      }
    }
  ]
}